echo "Start Create SPEC file"

name_paket=quagga-0.99.17
path_paket=TMP/my_packet
user=kosta
versionLtib=ltib-p2020rdb-20100428

cp /$path_paket/$name_paket.tar.gz /opt/freescale/pkgs/
mkdir /home/$user/$versionLtib/dist/lfs-5.1/$name_paket
cp /home/$user/$versionLtib/dist/lfs-5.1/template/template.spec /home/$user/$versionLtib/dist/lfs-5.1/$name_paket/$name_paket.spec




echo "Complete Create SPEC file"


